    "next-pwa": "^3.1.5",
    "polished": "^4.2.2",
    "react": "17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.1",
    "styled-components": "^5.2.1",
    "ts-toolbelt": "^9.6.0",
    "vercel": "^24.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-actions": "^6.0.28",
