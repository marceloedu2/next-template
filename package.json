{
  "name": "won",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "test": "jest",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "test:watch": "yarn test --watch",
    "storybook": "start-storybook -s ./public -p 6006 --no-dll",
